<div id="<%= dom_id audit_log_entry %>">
  <p>
    <strong>Action:</strong>
    <%= audit_log_entry.action %>
  </p>

  <p>
    <strong>Employee:</strong>
    <%= audit_log_entry.employee_id %>
  </p>

  <p>
    <strong>Record:</strong>
    <%= audit_log_entry.record_id %>
  </p>

  <p>
    <strong>Before state:</strong>
    <pre><%= audit_log_entry.before_state %></pre>
  </p>

  <p>
    <strong>After state:</strong>
    <pre><%= audit_log_entry.after_state %></pre>
  </p>

</div>
